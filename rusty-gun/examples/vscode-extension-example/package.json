{
  "name": "rusty-gun-extension-example",
  "displayName": "Rusty Gun Extension Example",
  "description": "Example VSCode extension using Rusty Gun",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:rusty-gun-example.helloWorld"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "rusty-gun-example.helloWorld",
        "title": "Hello World from Rusty Gun"
      },
      {
        "command": "rusty-gun-example.storeData",
        "title": "Store Data in Rusty Gun"
      },
      {
        "command": "rusty-gun-example.retrieveData",
        "title": "Retrieve Data from Rusty Gun"
      },
      {
        "command": "rusty-gun-example.searchData",
        "title": "Search Data with Vector Search"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "16.x",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "rusty-gun": "file:../.."
  }
}


