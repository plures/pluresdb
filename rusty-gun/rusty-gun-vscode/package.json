{
  "name": "rusty-gun-vscode",
  "displayName": "Rusty Gun Database",
  "description": "VSCode extension for Rusty Gun graph database with vector search",
  "version": "0.1.1",
  "publisher": "rusty-gun",
  "repository": {
    "type": "git",
    "url": "https://github.com/rusty-gun/rusty-gun.git"
  },
  "license": "MIT OR Commercial",
  "engines": {
    "vscode": "^1.104.0"
  },
  "categories": [
    "Database",
    "Other",
    "Machine Learning",
    "Visualization"
  ],
  "keywords": [
    "database",
    "graph",
    "vector-search",
    "p2p",
    "rusty-gun",
    "sqlite",
    "embeddings"
  ],
  "activationEvents": [
    "onCommand:rusty-gun.connect",
    "onCommand:rusty-gun.openDashboard",
    "onCommand:rusty-gun.executeQuery",
    "onCommand:rusty-gun.vectorSearch",
    "onView:rusty-gun.explorer",
    "onView:rusty-gun.vectorSearch",
    "onView:rusty-gun.graphView",
    "onLanguage:sql",
    "onLanguage:javascript",
    "onLanguage:typescript"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "rusty-gun.connect",
        "title": "Connect to Rusty Gun",
        "category": "Rusty Gun"
      },
      {
        "command": "rusty-gun.disconnect",
        "title": "Disconnect",
        "category": "Rusty Gun"
      },
      {
        "command": "rusty-gun.openDashboard",
        "title": "Open Dashboard",
        "category": "Rusty Gun"
      },
      {
        "command": "rusty-gun.executeQuery",
        "title": "Execute SQL Query",
        "category": "Rusty Gun"
      },
      {
        "command": "rusty-gun.vectorSearch",
        "title": "Vector Search",
        "category": "Rusty Gun"
      },
      {
        "command": "rusty-gun.createNode",
        "title": "Create Node",
        "category": "Rusty Gun"
      },
      {
        "command": "rusty-gun.createRelationship",
        "title": "Create Relationship",
        "category": "Rusty Gun"
      },
      {
        "command": "rusty-gun.refreshExplorer",
        "title": "Refresh Explorer",
        "category": "Rusty Gun"
      },
      {
        "command": "rusty-gun.exportGraph",
        "title": "Export Graph",
        "category": "Rusty Gun"
      },
      {
        "command": "rusty-gun.importGraph",
        "title": "Import Graph",
        "category": "Rusty Gun"
      }
    ],
    "views": {
      "explorer": [
        {
          "id": "rusty-gun.explorer",
          "name": "Rusty Gun Database",
          "when": "rusty-gun.connected"
        }
      ],
      "rusty-gun": [
        {
          "id": "rusty-gun.vectorSearch",
          "name": "Vector Search",
          "when": "rusty-gun.connected"
        },
        {
          "id": "rusty-gun.graphView",
          "name": "Graph View",
          "when": "rusty-gun.connected"
        },
        {
          "id": "rusty-gun.analytics",
          "name": "Analytics",
          "when": "rusty-gun.connected"
        }
      ]
    },
    "viewsContainers": {
      "rusty-gun": {
        "id": "rusty-gun",
        "title": "Rusty Gun",
        "icon": "resources/icon.svg"
      }
    },
    "menus": {
      "explorer/context": [
        {
          "command": "rusty-gun.createNode",
          "when": "rusty-gun.connected",
          "group": "rusty-gun"
        },
        {
          "command": "rusty-gun.createRelationship",
          "when": "rusty-gun.connected",
          "group": "rusty-gun"
        }
      ],
      "editor/context": [
        {
          "command": "rusty-gun.executeQuery",
          "when": "editorTextFocus && resourceScheme == file && resourceExtname == .sql",
          "group": "rusty-gun"
        },
        {
          "command": "rusty-gun.vectorSearch",
          "when": "editorTextFocus",
          "group": "rusty-gun"
        }
      ],
      "commandPalette": [
        {
          "command": "rusty-gun.connect",
          "when": "!rusty-gun.connected"
        },
        {
          "command": "rusty-gun.disconnect",
          "when": "rusty-gun.connected"
        },
        {
          "command": "rusty-gun.openDashboard"
        },
        {
          "command": "rusty-gun.executeQuery"
        },
        {
          "command": "rusty-gun.vectorSearch"
        },
        {
          "command": "rusty-gun.createNode"
        },
        {
          "command": "rusty-gun.createRelationship"
        },
        {
          "command": "rusty-gun.refreshExplorer"
        },
        {
          "command": "rusty-gun.exportGraph"
        },
        {
          "command": "rusty-gun.importGraph"
        }
      ],
      "view/title": [
        {
          "command": "rusty-gun.refreshExplorer",
          "when": "view == rusty-gun.explorer",
          "group": "navigation"
        },
        {
          "command": "rusty-gun.connect",
          "when": "view == rusty-gun.explorer && !rusty-gun.connected",
          "group": "navigation"
        }
      ]
    },
    "configuration": {
      "title": "Rusty Gun",
      "properties": {
        "rusty-gun.serverUrl": {
          "type": "string",
          "default": "http://localhost:34569",
          "description": "Rusty Gun server URL"
        },
        "rusty-gun.autoConnect": {
          "type": "boolean",
          "default": true,
          "description": "Automatically connect to server on startup"
        },
        "rusty-gun.vectorSearchThreshold": {
          "type": "number",
          "default": 0.3,
          "minimum": 0,
          "maximum": 1,
          "description": "Default threshold for vector search similarity"
        },
        "rusty-gun.maxResults": {
          "type": "number",
          "default": 100,
          "minimum": 1,
          "maximum": 1000,
          "description": "Maximum number of results to display"
        },
        "rusty-gun.enableNotifications": {
          "type": "boolean",
          "default": true,
          "description": "Enable notifications for database operations"
        },
        "rusty-gun.theme": {
          "type": "string",
          "enum": [
            "light",
            "dark",
            "auto"
          ],
          "default": "auto",
          "description": "Theme for Rusty Gun views"
        }
      }
    },
    "languages": [
      {
        "id": "rusty-gun-query",
        "aliases": [
          "Rusty Gun Query",
          "rgql"
        ],
        "extensions": [
          ".rgql",
          ".rusty-gun"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "rusty-gun-query",
        "scopeName": "source.rusty-gun-query",
        "path": "./syntaxes/rusty-gun-query.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "rusty-gun-query",
        "path": "./snippets/rusty-gun-query.json"
      },
      {
        "language": "sql",
        "path": "./snippets/sql.json"
      }
    ],
    "webview": {
      "rusty-gun.dashboard": {
        "title": "Rusty Gun Dashboard",
        "icon": "resources/dashboard.svg"
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "package": "vsce package",
    "install-extension": "code --install-extension rusty-gun-vscode-0.1.0.vsix"
  },
  "devDependencies": {
    "@types/node": "24.x",
    "@types/vscode": "^1.104.0",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.36.0",
    "typescript": "^5.9.3",
    "vsce": "^2.15.0"
  },
  "dependencies": {
    "axios": "^1.12.2",
    "cytoscape": "^3.33.1",
    "cytoscape-cose-bilkent": "^4.1.0",
    "cytoscape-dagre": "^2.5.0",
    "dagre": "^0.8.5",
    "monaco-editor": "^0.53.0"
  }
}
